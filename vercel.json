{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "headers": { "cache-control": "public, max-age=31536000, immutable" },
        "continue": true
      },
      {
        "handle": "filesystem"
      }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "dist"
  }